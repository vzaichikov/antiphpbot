{
    "name": "zrashwani/arachnid",
    "type": "library",
    "description": "A crawler to find all unique internal pages on a given website",
    "keywords": ["crawl","spider","search","scrape"],
    "homepage": "http://github.com/zrashwani/arachnid",
    "license": "MIT",
    "authors": [
        {
            "name": "Zeid Rashwani",
            "homepage": "http://zrashwani.com"
        },
        {
            "name": "Josh Lockhart",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-SPL": "*",
        "tightenco/collect": "^v8.34",
        "guzzlehttp/psr7": "^1.4",
        "symfony/panther": "^1.0",
        "fabpot/goutte": "^4.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^v4.4",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^7.5|^8.5|^9.2",
        "monolog/monolog": "^1.22",
        "dbrekelmans/bdi": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Arachnid\\": ["src/Arachnid/", "tests/src/"]
        }
    }
}
